{%
  set classes = [
  'dcf-bleed dcf-wrapper dcf-pt-8 dcf-pb-8 unl-bg-lighter-gray dcf-mt-10 dcf-mb-10',
  'block',
  'block-' ~ configuration.provider|clean_class,
  'block-' ~ plugin_id|clean_class,
]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
