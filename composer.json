{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/unlcms/project-herbie-composer-plugin.git"
        },
        {
            "type": "package",
            "package": {
                "name": "unl/wdntemplates",
                "version": "5.1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/unl/wdntemplates.git",
                    "reference": "5.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "robinherbots/jquery.inputmask",
                "version": "4.0.6",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/robinherbots/jquery.inputmask/archive/4.0.6.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jackocnr/intl-tel-input",
                "version": "15.0.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/jackocnr/intl-tel-input/archive/v15.0.1.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ractoon/jquery-text-counter",
                "version": "0.8.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/ractoon/jquery-text-counter/archive/0.8.0.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "nigelotoole/progress-tracker",
                "version": "1.4.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/NigelOToole/progress-tracker/archive/v1.4.0.zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/allowed_formats": "^1.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/block_content_permissions": "^1.6",
        "drupal/block_form_alter": "^1.0",
        "drupal/block_type_templates": "^1.0@alpha",
        "drupal/body_scroll_lock": "^1.0",
        "drupal/chosen": "^2.7",
        "drupal/ckeditor_config": "^3.0",
        "drupal/config_filter": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/config_role_split": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "^3.0",
        "drupal/dcf": "^1.0@beta",
        "drupal/diff": "^1.0",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_embed": "^1.0@RC",
        "drupal/entity_reference_revisions": "^1.0",
        "drupal/entity_usage": "2.x-dev",
        "drupal/external_entities": "^2.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/honeypot": "^1.0",
        "drupal/image_effects": "^2.3",
        "drupal/imagemagick": "^2.4",
        "drupal/inline_block_title_automatic": "^1.0@alpha",
        "drupal/inline_entity_form": "^1.0",
        "drupal/layout_builder_modal": "^1.0",
        "drupal/layout_builder_restrictions": "^2.0",
        "drupal/linkit": "^5.0",
        "drupal/markup": "^1.0@beta",
        "drupal/media_entity_browser": "2.x-dev#ff5532ec8a7a8aff89415624de1b17d7697557ad",
        "drupal/media_library_extras": "^1.0@alpha",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.5",
        "drupal/token": "^1.0",
        "drupal/twig_tweak": "^2.0",
        "drupal/webform": "^5.2",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "jackocnr/intl-tel-input": "15.0.1",
        "nigelotoole/progress-tracker": "1.4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "ractoon/jquery-text-counter": "0.8.0",
        "robinherbots/jquery.inputmask": "4.0.6",
        "unl/wdntemplates": "^5.1",
        "unlcms/external_entities_unldirectory": "dev-8.x-2.x",
        "unlcms/project-herbie-composer-plugin": "^1.0",
        "unlcms/unl_breadcrumbs": "^1.0",
        "unlcms/unl_cas": "dev-8.x-1.x",
        "unlcms/unl_five": "^5.1",
        "unlcms/unl_multisite": "dev-8.x-1.x",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
        "drupal/config_inspector": "^1.0@beta",
        "drupal/core-dev": "^8.8",
        "drupal/twig_xdebug": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/UnlScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "cd web && patch -p1 < ../patches/custom-index.php.patch",
            "cd web && patch -p1 < ../patches/custom-htaccess.patch",
            "cd web && patch -p1 < ../patches/custom-unl_multisite-example.sites.php.patch"
        ],
        "install-wdn": "DrupalProject\\composer\\UnlScriptHandler::deployWdn"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["library"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/libraries/jquery.inputmask": ["robinherbots/jquery.inputmask"],
            "web/libraries/jquery.intl-tel-input": ["jackocnr/intl-tel-input"],
            "web/libraries/jquery.textcounter": ["ractoon/jquery-text-counter"],
            "web/libraries/progress-tracker": ["nigelotoole/progress-tracker"]
        },
        "patches": {
            "drupal/core": {
                "2827910 - Add Option to Display Top Level page in BookNavigationBlock": "patches/allow-top-item-to-show-2827910-26-D8.patch",
                "3056149 - Include 'is_active' value in book tree items": "patches/drupal-book_tree_active_item-3056149-9.patch",
                "502430 - Allow the book outline functionality on non-book-enabled node types to be hidden": "patches/core_book-outline_permission-502430-80.patch",
                "3008119 - Provide hook_oembed_providers_alter()": "patches/drupal-allow_alter_oembed_providers-3008119-4.patch",
                "3060292 - Drupal\\media\\Entity\\Media::prepareSave should convert URL object metadata to string before saving" : "patches/drupal_media-resource_convert_url_object_to_string-3060292-5.patch",
                "Custom - Allow '@' in CSS selectors for Views displays": "patches/custom-views-DisplayPluginBase-valid-css-selector.patch",
                "3080606 - Re-order Layout Builder sections": "patches/3080606-reorder-layout-sections-28.patch",
                "3045171 - Form blocks rendered inside layout builder break save": "patches/layout-builder-save-issue-3045171-140.patch",
                "2897377 - data-drupal-selector is set to random value": "patches/drupal-data_drupal_selector-2897377-20.patch",
                "3092795 - Restore styling for embedded media edit button": "patches/3092795-22.patch"
            },
            "drupal/block_content_permissions": {
                "2920739 - Allow accessing the 'Custom block library' page without 'Administer blocks' permission": "patches/block_content_permissions-access_listing_page-2920739-23.patch"
            },
            "drupal/block_type_templates": {
                "3018307 - Support for View Modes": "https://www.drupal.org/files/issues/2019-03-21/block_type_templates-view_mode_support-3018307-4.patch"
            },
            "drupal/chosen": {
                "3062839 - UI (CSS styling) is broken in off-canvas settings tray": "patches/chosen-offcanvas-css-improvements-3062839-5.patch"
            },
            "drupal/config_ignore": {
                "2857247 - Support for export filtering via Drush": "https://www.drupal.org/files/issues/2018-04-19/support-for-export-2857247-18.patch"
            },
            "drupal/entity_embed": {
                "3061050 - \"TypeError: originalGetFocusedWidget is not a function\" if drupalimage plugin button not used": "patches/originalgetfocusedwidget-not-function-3061050-2.patch"
            },
            "drupal/external_entities": {
                "3065268 - external_entities_inline_entity_form_entity_form_alter() needs to check entity instance sooner": "patches/external_entities-do_not_break_ief_render_elements-3089287-4.patch"
            },
            "drupal/linkit": {
              "3063393 - Remove '/edit' from media entity paths": "patches/linkit-media_entity_update_canonical_path-3063393-11.patch"
            },
            "drupal/media_library_extras": {
                "3108619 - Allow View Mode Options to be Restricted on a Per-bundle Basis for Media Embed": "patches/media_library_extras-media_embed_view_mode_restrictions-3108619-10.patch"
            },
            "drupal/paragraphs": {
                "2901390 - Integrity constraint violation: 1048 Column 'langcode' cannot be null": "patches/2901390-n24.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": {
                  "append": "patches/default.settings.php.append.php"
                }
            },
            "locations": {
                "web-root": "web/"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
